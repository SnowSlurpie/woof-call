<div class="login-sheet">
    <h2 class="page-title">Signup</h2>
{{!--Form for Dog Info--}}
    <form class="form signup-form">
        <label for = "dogName">Dog's Name:</label><br>
        <input type="text" id="dogName" name="dogName" placeholder="Dog's Name Here"><br>
        <label for = "dogBreed">Dog's Breed:</label><br>
        <input type="text" id="dogBreed" name="dogBreed" placeholder="Dog's Breed Here"><br>
        <label for = "dogAge">Dog's Age:</label><br>
        <imput type="text" id="dogAge" name="dogAge" placeholder="Dog's Age Here"><br>
        <input type="radio"  id="sexM" name="sex" value="Male"> <label for="Male">Male</label><br>
        <input type="radio"  id="sexF" name="sexF" value="Female"> <label for="Female">Female</label><br>
{{!--Form for Owner Info --}}
        <input type="text" id="ownerName" name="ownerName" placeholder="Your Name Here">What is your name?</label><br>
        <input type="number" id="zipCode" name="zipCode" placeholder="Your Zip Code Here">What is your zip code?</label><br>
    </form> 

    {{!-- Start here w/ Email/Password section of SignUp Page --}}
    <form class="form signup-form">
        <div class="form-group">
         <label for="email-signup">Email:</label>
         <input class="form-input" type="text" id="email-signup" />
        </div>
        <div class="form-group"> 
            <label for="password-signup">Password:</label>
            <input class="form-input" type="password" id="password-signup" />
        </div>
        <div class="form-group">
            <button class="btn btn-primary" type="submit">signup</button>
        </div>
    </form>
</div>